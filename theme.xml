<view name="system">  
<text name="logoText">
<fontPath>./art/sistemas/fuentes/.ttf</fontPath>
<color>${baseColor}</color>
<fontSize>0.060</fontSize>
<forceUppercase>true</forceUppercase>
</text>
<image name="logo">
<path>./${system.theme}/art/system-${lang}.png</path>
